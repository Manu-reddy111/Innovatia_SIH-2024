<!DOCTYPE html>
<html lang="en" data-theme="light">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<!-- Boxicons -->
	<link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
	<link rel="stylesheet"
	href="https://unpkg.com/boxicons@latest/css/boxicons.min.css">
	<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" rel="stylesheet">

	<!-- My CSS -->
	{{!-- <link rel="stylesheet" href="/css/style.css"> --}}
	<link rel="stylesheet" href="css/home.css">
	<link
    href="https://cdn.jsdelivr.net/npm/remixicon@4.5.0/fonts/remixicon.css"
    rel="stylesheet"
/>
	<script src='https://kit.fontawesome.com/a076d05399.js' crossorigin='anonymous'></script>

	<title>Admin-Innovatia</title>
</head>
<body>


	<!-- SIDEBAR -->
	<section id="sidebar" class="hide">
		<a href="/AdminHomepage" class="brand">
			<i class='bx bxs-dashboard'></i>
			<span class="text">Innovatia</span>
		</a>
		
		<ul class="side-menu">
			<li class="active">
				<a href="/AdminHomepage" > <!-- matches contentMap['home'] -->

					<i class='bx bxs-home-alt-2'></i>
					<span class="text">Home</span>
				</a>
			</li>
			
			<li>
				<a href="/AdminLeaderboard" >
					<i class='bx bxs-bar-chart-alt-2'></i>
					<span class="text">Leaderboard</span>
				</a>
			</li>
			<li>
				<a href="/AdminManageStudent" >
					<i class='bx bx-user'></i>
					
					<span class="text">Manage Students</span>
				</a>
			</li>
			<li>
				<a href="/AdminManageFaculty" >
					<i class='bx bx-user-voice' ></i>
					<span class="text">Manage Faculty</span>
				</a>
			</li>
			<li>
				<a href="/requestsstaffpublication" >
					<i class='bx bx-user-plus'></i>
					<span class="text">Faculty Publications Requests</span>
				</a>
			</li>
			
			<li>
				<a href="/requestsstudentproject" >
					<i class='bx bxs-folder-open' ></i>
					
					<span class="text">Student Innovation Requests</span>
				</a>
			</li>
			<li>
				<a href="/Logout" class="/Logout">
					<i class='bx bxs-log-out-circle' ></i>
					<span class="text">Logout</span>
				</a>
			</li>
		</ul>
	</section>
	<!-- SIDEBAR -->



	<!-- CONTENT -->
	<section id="content">
		<!-- NAVBAR -->
		<nav>
			<i class='bx bx-menu' ></i>
			<a href="#" class="nav-link">Categories</a>
			<form action="#">
				<div class="form-input">
					<input type="search" placeholder="Search...">
					<button type="submit" class="search-btn"><i class='bx bx-search' ></i></button>
				</div>
			</form>
			<input type="checkbox" id="switch-mode" hidden>
			<label for="switch-mode" class="switch-mode"></label>
			<a href="/notification" class="notification">
				<i class='bx bxs-bell' ></i>
				<span class="num">8</span>
			</a>
			
				<!-- Assuming this is in your navbar -->
<nav>
    <div class="profile" id="profile">
        <img src="img/people.png" alt="Profile Picture" class="profile-img">
    </div>
    <div id="profileMenu" class="profile-menu">
        <ul>         
            <li><a href="/Logout">Logout</a></li>
        </ul>
    </div>
</nav>

			









		</nav>
		<!-- NAVBAR -->

		<!-- MAIN -->
		<main>
			<div class="head-title">
				<div class="left">
					<h1>Home</h1>
					
				</div>
				<div class="right">
					<div class="right-part1">
						<a href="/AdminCollegeAnalytics">
							<i class='bx bxs-doughnut-chart' ></i>
							<span class="text">Analytics</span>
						</a>
						<a href="/AdminCollegeAwards" >
							<i class='bx bxs-award'></i>
							
							<span class="text">Awards</span>
						</a>
						<a href="/AdminCollegeGrants" >
							<i class='bx bxs-bank' ></i>
							
							<span class="text">Grants</span>
						</a>
						
						<a href="/AdminCollegeStartup" >
							<i class='bx bxs-buildings'></i>
							
							
							<span class="text">Startup incubated</span>
						</a>
					</div>
					 
				</div>
				
			</div>

			<ul class="box-info">
				<li>
					<i class='bx bx-folder'></i>
					<span class="text">
						<h3>{{countproject}}</h3>
						<p>Projects</p>
					</span>
				</li>
				<li>
					<i class='bx bx-file-blank'></i>
					<span class="text">
						<h3>{{countresearch}}</h3>
						<p>Research papers</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-notepad'></i>
					<span class="text">
						<h3>{{countpatent}}</h3>
						<p>Patents</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-bank' ></i>
					
					<span class="text">
						<h3>{{countpublication}}</h3>
						<p>Grants</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-buildings'></i>
					<span class="text">
						<h3>2</h3>
						<p>startups incubated</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-award'></i>
					
					<span class="text">
						
						<h3>400</h3>
						<p>Awards</p>
					</span>
				</li>
				
			</ul>


			<div class="table-data">
				<div class="order">
					<div class="head">
						<h3>Recent  Student Projects</h3>
						
						
						<i class='bx bx-search' ></i>
						<i class='bx bx-filter' ></i>
					</div>
					{{#if Message}}
        <p class="{{#if isError}}error-message{{else}}success-message{{/if}}">
            {{Message}}
        </p>
        {{/if}}
		
		{{#if Projects.length}}
					<table>
						<thead>
							<tr>
								<th>S.no</th>
								<th>User</th>
								<th>User Id</th>
								<th>Date Order</th>
								<th>Project Title</th>
								<th>Team</th> 
								<th>Dept</th>
								<th>Status</th>
								<th>project type</th>
								<th>Rating</th> 
								<th>Delete</th> </tr>
						</thead>
						<tbody>
							{{#each Projects}}
							<tr style="cursor: pointer;">

								<td>
									<span>{{increment @index}}</span>
								</td>
								<td class="std-profile">
									
									<p> <span><img src="{{this.profilePic}}"></span>{{this.Username}}</p>
								</td>
								<td>{{this.UserId}}</td>
								<td>{{this.Uploaddate}}</td>
								<td>
									<span>{{this.ProjectTitle}}</span>
								</td>
								<td>
									<span>{{this.TeamName}}</span>
								</td>
								<td>
									{{this.Dept}}
								</td>
								<td>
									 {{#if (eq Status "Completed")}}
            <span class="status completed">Completed</span>
          {{else if (eq Status "Pending")}}
            <span class="status pending">Pending</span>
          {{else if (eq Status "Accepted")}}
            <span class="status process">Accepted</span>
          {{else if (eq Status "Rejected")}}
            <span class="status rejected">Rejected</span>
          {{else}}
            <span class="status unknown">Unknown</span>
          {{/if}}
								</td>

								<td><span class="project-type">{{this.ProjectType}}</span></td>
								<td>
									
									<span class="star-rating">

 					{{#if (eq Rating "0")}}
            			<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "1")}}
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "2")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "3")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "4")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "5")}}
          			  	<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
          			{{else}}
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{/if}}
									</span>
								</td> 
								<td> 
										
                                <form action="/AdminProjectDelete" method="post">
								 <input type="hidden" name="id" value="{{this.id}}">
  								 <button type="submit" class="submit">
                                      <i class="ri-delete-bin-6-line"></i>
								</button>
								</form>	
									
								</td>
								

							</tr>
							    {{/each}}
						</tbody>
					</table> 
					{{else}}
        				<p>No user data found.</p>
    				{{/if}}
				</div>
				
			</div>

			
<div class="table-data">
				<div class="order">
					<div class="head">
						<h3>Recent Staff Publications</h3>
						<i class='bx bx-search' ></i>
						<i class='bx bx-filter' ></i>
					</div>  
					{{#if Uploadd.length}}
					<table>
						<thead>
							<tr>
								<th>S.no</th>
								<th>User</th>
								<th>User Id</th> 
								<th>Date Order</th>
								<th>Project Title</th> 
								<th>Dept</th>
								<th>Status</th>
								<th>project type</th>  
								<th>Rating</th> 
								<th>Delete</th> 
							</tr>
						</thead>
						<tbody>{{#each Uploadd}}
							<tr   style="cursor: pointer;">

								<td >
									<span>{{increment @index}}  </span>
								</td>
								<td >
									
									<p> <span><img src="{{this.profilePic}}"></span>{{this.Username}}</p>
								</td>
								<td>{{this.UserId}}</td>
								<td>{{this.Uploaddate}}</td>
								<td > <span>{{this.ProjectTitle}}</span> </td>
								
								<td> {{this.Dept}} </td>
								<td>
										 {{#if (eq Status "Completed")}}
            <span class="status completed">Completed</span>
          {{else if (eq Status "Pending")}}
            <span class="status pending">Pending</span>
          {{else if (eq Status "Accepted")}}
            <span class="status process">Accepted</span>
          {{else if (eq Status "Rejected")}}
            <span class="status rejected">Rejected</span>
          {{else}} 
            <span class="status unknown">Unknown</span>
          {{/if}}


								</td>

								<td >
                                    <span class="project-type">{{this.ProjectType}}</span></td>
								 
<td>
  <span class="star-rating">

 					{{#if (eq Rating "0")}}
            			<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "1")}}
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "2")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "3")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "4")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "5")}}
          			  	<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
          			{{else}}
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{/if}}
									</span>
</td>

								<td>
									  
									   <form action="/AdminPublicatioDelete" method="post">
								 <input type="hidden" name="id" value="{{this.id}}">
  								 <button type="submit" class="submit">
                                      <i class="ri-delete-bin-6-line"></i>
								</button>
								</form>	

								</td>
								

							</tr> {{/each}}
						</tbody>
					</table> 
					{{else}}
        				<p>No user data found.</p>
    				{{/if}}
		</main>
		<!-- MAIN -->
	</section>
	

	<script src="js/script.js"></script>
</body>
</html>