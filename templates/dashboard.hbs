<!DOCTYPE html>
<html lang="en" data-theme="light">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<!-- Boxicons -->
	<link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
	<link rel="stylesheet"
	href="https://unpkg.com/boxicons@latest/css/boxicons.min.css">
	<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" rel="stylesheet">

	<!-- My CSS -->
	<link rel="stylesheet" href="css/home.css">
	<link
    href="https://cdn.jsdelivr.net/npm/remixicon@4.5.0/fonts/remixicon.css"
    rel="stylesheet"
/>
	<link rel="stylesheet" href="css/dashboard.css">

	<title>Student-Dashboard</title>
</head>
<body>


	<!-- SIDEBAR -->
	<section id="sidebar" class="hide">
		<a href="/dashboard" class="brand">
			<i class='bx bxs-dashboard'></i>
			<span class="text">{{student.username}} Dashboard</span>
		</a>
		
		<ul class="side-menu">
			<li >
				<a href="/homepage" > <!-- matches contentMap['home'] -->

					<i class='bx bxs-home-alt-2'></i>
					<span class="text">Home</span>
				</a>
			</li>
			<li class="active">
				<a href="/dashboard" >
					<i class='bx bxs-dashboard'  ></i>
					<span class="text">Dashboard</span>
				</a>
			</li>
			<li >
				<a href="/recentpublications" >
					<i class='bx bxs-folder-open' ></i>
					<span class="text">Publications</span>
				</a>
			</li>
			<li>
				<a href="/leaderboard" >
					<i class='bx bxs-bar-chart-alt-2'></i>
					<span class="text">Leaderboard</span>
				</a>
			</li>
			
			
		
			
			<li>
				<a href="/recentachievements" >
					<i class='bx bxs-award'></i>
					<span class="text">Achievements</span>
				</a>
			</li>
			<li>
				<a href="/recentgrants" >
					<i class='bx bxs-bank' ></i>
					<span class="text">Grants</span>
				</a>
			</li>
			<li>
				<a href="/recentstartups" >
					<i class='bx bxs-buildings'></i>
					<span class="text">startupincubated</span>
				</a>
			</li>
			
		</ul>
		<ul class="side-menu" >
			<li>
				<a href="/Settings">
					<i class='bx bxs-cog' ></i>
					<span class="text">Settings</span>
				</a>
			</li>
			<li>
				<a href="/Logout" class="/Logout">
					<i class='bx bxs-log-out-circle' ></i>
					<span class="text">Logout</span>
				</a>
			</li>
		</ul>
	</section>
	<!-- SIDEBAR -->



	<!-- CONTENT -->
	<section id="content">
		<!-- NAVBAR -->
		<nav>
			<i class='bx bx-menu' ></i>
			<a href="#" class="nav-link">Categories</a>
			<form action="#">
				<div class="form-input">
					<input type="search" placeholder="Search...">
					<button type="submit" class="search-btn"><i class='bx bx-search' ></i></button>
				</div>
			</form>
			<input type="checkbox" id="switch-mode" hidden>
			<label for="switch-mode" class="switch-mode"></label>
			<a href="/notification" class="notification">
				<i class='bx bxs-bell' ></i>
				<span class="num">8</span>
			</a>
			
				<!-- Assuming this is in your navbar -->
<nav>
    <div class="profile" id="profile">
        <img src="{{student.profilePic}}" alt="Profile Picture" class="profile-img">
    </div>
    <div id="profileMenu" class="profile-menu">
        <ul>
            <li><a href="#">Profile</a></li>          
            <li><a href="/Logout">Logout</a></li>
        </ul>
    </div>
</nav>

			









		</nav>
		<!-- NAVBAR -->

		<!-- MAIN -->
		<main>
			<div class="head-title">
				<div class="left">
					<h1> {{student.username}} - Dashboard</h1>
					<ul class="breadcrumb">
						<li>
							<a href="#">Dashboard</a>
						</li>
						<li><i class='bx bx-chevron-right' ></i></li>
						<li>
							<a class="active" href="#">Home</a>
						</li>
					</ul>
				</div>
				<div class="right">
					<div class="right-part1">
						<a href="./studentanalytics.hbs">
							<i class='bx bxs-doughnut-chart' ></i>
							<span class="text">Analytics</span>
						</a>
						<a href="./studentawards.hbs" >
							<i class='bx bxs-award'></i>
							
							<span class="text">Awards</span>
						</a>
						<a href="./recentgrants.hbs" >
							<i class='bx bxs-bank' ></i>
							
							<span class="text">Grants</span>
						</a>
						
						<a href="./recentstartups.hbs" >
							<i class='bx bxs-buildings'></i>
							
							
							<span class="text">Startup incubated</span>
						</a>
					</div>
					
				</div>
				
			</div>
			<div class="studentprofile">
				<div class="studentprofile-top">
					
					<div class="studentprofile-left">
						<h3>Profile</h3>
						<div class="student-icons">
							<a href="{{UploaddProfile.linkedinLink}}" class="linkedin-link" target="_blank"><i class="bx bxl-linkedin"></i></a>
							<a href="{{UploaddProfile.githubLink}}" class="github-link" target="_blank"><i class="bx bxl-github"></i></a>
							<a href="{{UploaddProfile.facebookLink}}" class="facebook-link" target="_blank"><i class="bx bxl-facebook"></i></a>
						</div>
						
					</div>
					<div class="studentprofile-right">
						<i class='bx bxs-edit-alt'></i>
					</div>
				</div>

				<div class="studentprofile-parts">
					<div class="studentprofile-part1">
					<img src="{{UploaddProfile.backgroundImage}}">
					</div>
					<div class="studentprofile-part2">
						<div class="student-usernamepic">
							<span><img src="{{student.profilePic}}"></span>
							<p>{{student.username}}</p>
							
							


						</div>
						<div class="student-data1">
							<p> <span><i class='bx bxs-invader' ></i></span>{{student.department}}</p>
							<p> <span><i class='bx bxs-hard-hat' ></i></span>{{student.hallticket}}</p>

						</div>
						<div class="student-data2">
							<p> <i class='bx bxs-award' ></i></span> {{UploaddProfile.awards}}     <span><i class='bx bxl-google' ></i></span></p>
							 
						</div>
						<div class="student-data3">
							<p> <i class='bx bx-badge'></i>  {{UploaddProfile.wins}}  </p> 
						</div>
						<div class="student-data4">
							<p><i class="ri-trophy-fill"></i> {{UploaddProfile.awards}} </p>
							<p> <i class="ri-trophy-fill"></i> {{UploaddProfile.wins}} </p>
						</div>
						<div class="student-data5">
							<p>    {{UploaddProfile.description}}       </p>
						</div>
						

						
	
					</div>
				</div>
				<!-- Edit Form (Initially Hidden) -->
<div id="edit-form-container" style="display: none;">
   <form action="/submitprofile" method="POST" id="editform">
  <h3>Edit Profile</h3>

  <!-- Links Section -->
  <label for="github-link">GitHub:</label>
  <input type="url" name="githubLink" id="github-link" placeholder="Enter GitHub URL" required>
  <label for="linkedin-link">LinkedIn:</label>
  <input type="url" name="linkedinLink" id="linkedin-link" placeholder="Enter LinkedIn URL" required>
  <label for="facebook-link">Facebook:</label>
  <input type="url" name="facebookLink" id="facebook-link" placeholder="Enter Facebook URL" required>

  <!-- Images Section -->
  <label for="background-image">Background Image:</label>
  <input type="url" name="backgroundImage" id="background-image" placeholder="Enter Background Image URL" required>
  <label for="profile-image">Profile Image:</label>
  <input type="url" name="profileImage" id="profile-image" placeholder="Enter Profile Image URL" required>

  <!-- Department and Hall Ticket -->
  <label for="department">Department:</label>
  <input type="text" name="department" id="department" placeholder="Enter Department" required>
  <label for="hall-ticket">Hall Ticket Number:</label>
  <input type="text" name="hallTicket" id="hall-ticket" placeholder="Enter Hall Ticket Number" required>

  <!-- Dynamic Awards Section -->
  <div id="awards-section">
    <h4>Awards:</h4>
    <div class="dynamic-field">
      <input type="text" name="award[]" placeholder="Enter Award" required> 
    </div>
  </div>

  <!-- Dynamic Wins Section -->
  <div id="wins-section">
    <h4>Wins:</h4>
    <div class="dynamic-field">
      <input type="text" name="win[]" placeholder="Enter Win" required> 
    </div>
  </div>

  <!-- Description -->
  <label for="description">Description:</label>
  <textarea name="description" id="description" rows="5" placeholder="Enter Description" required></textarea>

  <!-- Submit Button -->
      <input type="submit" name="submit"> 
</form>

   
</div>

				
			</div>

			<ul class="box-info">
				<li>
					<i class='bx bx-folder'></i>
					<span class="text">
						<h3>{{pubCount}}</h3>
						<p>Projects</p>
					</span>
				</li>
				<li>
					<i class='bx bx-file-blank'></i>
					<span class="text">
						<h3>28</h3>
						<p>Research papers</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-notepad'></i>
					<span class="text">
						<h3>43</h3>
						<p>Patents</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-bank' ></i>
					
					<span class="text">
						<h3>4</h3>
						<p>Grants</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-buildings'></i>
					<span class="text">
						<h3>2</h3>
						<p>startups incubated</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-award'></i>
					
					<span class="text">
						
						<h3>400</h3>
						<p>Awards</p>
					</span>
				</li>
				
			</ul>


			<div class="table-data">
				<div class="order">
					<div class="head">
						<h3>Recent Projects</h3>
						<i class='bx bx-search' ></i>
						<i class='bx bx-filter' ></i>
					</div>
					{{#if Uploadd.length}}
					<table>
						<thead>
							<tr>
								<th>S.no</th>
								<th>User</th>
								<th>User Id</th>
								<th>Date Order</th>
								<th>Project Title</th>
								<th>Team</th> 
								<th>Dept</th>
								<th>Status</th>
								<th>project type</th>
								<th>Rating</th>
								<th>Edit</th>
								<th>Delete</th> </tr>
						</thead>
						<tbody>
							{{#each Uploadd}}
							<tr onclick="location.href='/viewproject/{{this.Uploadpublicationid}}'" style="cursor: pointer;">

								<td>
									<span>{{increment @index}}</span>
								</td>
								<td class="std-profile">
									
									<p> <span><img src="{{this.profilePic}}"></span>{{this.Username}}</p>
								</td>
								<td>{{this.UserId}}</td>
								<td>{{this.Uploaddate}}</td>
								<td>
									<span>{{this.ProjectTitle}}</span>
								</td>
								<td>
									<span>{{this.TeamName}}</span>
								</td>
								<td>
									{{this.Dept}}
								</td>
								<td>
									 {{#if (eq Status "Completed")}}
            <span class="status completed">Completed</span>
          {{else if (eq Status "Pending")}}
            <span class="status pending">Pending</span>
          {{else if (eq Status "Accepted")}}
            <span class="status process">Accepted</span>
          {{else if (eq Status "Rejected")}}
            <span class="status rejected">Rejected</span>
          {{else}} 
            <span class="status unknown">Unknown</span>
          {{/if}}
								</td>

								<td><span class="project-type">{{this.ProjectType}}</span></td>
								<td>
									
									<span class="star-rating">

 					{{#if (eq Rating "0")}}
            			<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "1")}}
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "2")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "3")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "4")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Rating "5")}}
          			  	<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
          			{{else}}
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{/if}}
									</span>
								</td>
								<td>
									<a href="publications.html"><i class="ri-edit-fill"></i></a>
								</td>
								<td>
									<a href="#"><i class="ri-delete-bin-6-line"></i></a>
								</td>
								

							</tr>
							    {{/each}}
						</tbody>
					</table> 
					{{else}}
        				<p>No user data found.</p>
    				{{/if}}
				</div>
				
			</div>
			
		</main>
		<!-- MAIN -->
	</section>
	

	<script src="js/script.js"></script>
	<script src="js/dashboard.js"></script>
</body>
</html>