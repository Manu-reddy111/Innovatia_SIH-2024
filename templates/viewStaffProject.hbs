<!DOCTYPE html>
<html lang="en" data-theme="light">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<!-- Boxicons -->
	<link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
	<link rel="stylesheet"
	href="https://unpkg.com/boxicons@latest/css/boxicons.min.css">
	<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" rel="stylesheet">

	<!-- My CSS -->
	<link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/viewproject.css">
    <link
    href="https://cdn.jsdelivr.net/npm/remixicon@4.5.0/fonts/remixicon.css"
    rel="stylesheet"
/>

	<title>Staff - View Projects</title>
</head>
<body>


	<!-- SIDEBAR -->
	<section id="sidebar" class="hide">
		<a href="/StaffHomepage" class="brand">
			<i class='bx bxs-dashboard'></i>
			<span class="text">Innovatia</span>
		</a>
		
		<ul class="side-menu">
			<li class="active">
				<a href="/StaffHomepage" > <!-- matches contentMap['home'] -->

					<i class='bx bxs-home-alt-2'></i>
					<span class="text">Home</span>
				</a>
			</li>
			<li>
				<a href="/staffDashboard" >
					<i class='bx bxs-dashboard'  ></i>
					<span class="text">Dashboard</span>
				</a>
			</li>
			
			
			
			
			<li>
				<a href="/StaffProjectRequests" >
					<i class='bx bxs-folder-open' ></i>
					
					<span class="text">Project Requests</span>
				</a>
			</li>
			<li>
				<a href="/StaffLeaderboard" >
					<i class='bx bxs-bar-chart-alt-2'></i>
					<span class="text">Leaderboard</span>
				</a>
			</li>
			<li>
				<a href="/StaffRecentPublications" >
					<i class='bx bxs-folder-open' ></i>
					
					<span class="text">Publications</span>
				</a>
			</li>  
			<li>
				<a href="/SLogout" class="logout">
					<i class='bx bxs-log-out-circle' ></i>
					<span class="text">Logout</span>
				</a>
			</li>
		</ul>
	</section>
	<!-- SIDEBAR -->



	<!-- CONTENT -->
	<section id="content">
		<!-- NAVBAR -->
		<nav>
			<i class='bx bx-menu' ></i>
			<a href="#" class="nav-link">Categories</a>
			<form action="#">
				<div class="form-input">
					<input type="search" placeholder="Search...">
					<button type="submit" class="search-btn"><i class='bx bx-search' ></i></button>
				</div>
			</form>
			<input type="checkbox" id="switch-mode" hidden>
			<label for="switch-mode" class="switch-mode"></label>
			<a href="/notification" class="notification">
				<i class='bx bxs-bell' ></i>
				<span class="num">8</span>
			</a>
			
				<!-- Assuming this is in your navbar -->
<nav>
    <div class="profile" id="profile">
        <img src="/{{Staff.profilePic}}" alt="Profile Picture" class="profile-img">
    </div>
    <div id="profileMenu" class="profile-menu">
        <ul>
            <li><a href="#">Profile</a></li>          
            <li><a href="/SLogout">Logout</a></li>
        </ul>
    </div>
</nav>

				









		</nav>
		<!-- NAVBAR -->

		<!-- MAIN -->
		<div class="main2">
            <div class="viewproject-part1">
                <div class="title-project-std">
                    <h4 class="project-title">{{Projects.Username}} -Innovatia </h4>
                    <p> <i class='bx bx-badge'></i> Golden Badge 2024</p>

                </div>
                    <div class="viewproject-part1-p1" >
                        <img class="thumbnail-image"  src="/{{Uploadpub.thumbnailFile}}" alt="">

                    </div>
                    <div class="project-links">
                        <a href="{{Uploadpub.GitHub}}" ><i class="bx bxl-github"></i>GitHub</a>
                        <a href="{{Uploadpub.Live}}" >Live Demo</a> 
                    </div>
                    <div class="upload-images">
                        {{#each Uploadpub.imageFiles}} 
                            
                        <div class="upload-images-part1">
                            <img src="/{{this}}" alt="">
                            
                        </div> 
                        {{/each}}

                    </div>
                    <div class="researchpapers">
                         {{#each Uploadpub.researchPapers}}  
                        <li>
                            <i class='bx bx-file-blank'></i>
                            <span class="text"> <p>
                                <a href="/{{this}}">Research paper's</a> </p> </span>
                        </li>{{/each}} 

                    </div>
                    
                   
                    <div class="patents">
                        
                         {{#each Uploadpub.patentDocs}}  
                        <li>
                            <i class='bx bxs-notepad'></i>
                            <span class="text"> <p>
                                <a href="/{{this}}">Patent Doc</a>  </p></span>
                        </li>{{/each}}  

                    </div>
                    <div class="project-zip">
                         {{#each Uploadpub.projectFiles}}  
                        <li>
                            <i class='bx bx-folder'></i>
                            <span class="text">  <p>
                                <a href="/{{this}}">Project Zip </a>   </p>   </span>
                        </li>{{/each}} 
                         

                    </div>
                    <div class="other-certificates">
                          {{#each Uploadpub.projectFiles}}
                        <li>
                            <i class='bx bx-certification' ></i>
                            <span class="text"> 
                                 <p> <a href="/{{this}}">Certificate</a> </p>
                            </span>
                        </li>{{/each}}  

                    </div>
                    

                
            </div>
            <div class="viewproject-part2">
                <div class="student-usernamepic">
                   <div class="std-profile">
                    <span><img src="/{{Projects.profilePic}}"></span>
                    <p> {{Projects.Username}}  </p>
                   </div>
                    <span class="star-rating">
 					{{#if (eq Projects.Rating "0")}}
            			<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Projects.Rating "1")}}
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Projects.Rating "2")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Projects.Rating "3")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Projects.Rating "4")}}
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
		  				<i class="fas fa-star"></i>
						<i class="far fa-star"></i>
          			{{else if (eq Projects.Rating "5")}}
          			  	<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
						<i class="fas fa-star"></i>
          			{{else}}
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
						<i class="far fa-star"></i>
          			{{/if}} 
                    </span>

                    
                    


                </div>
                
                <div class="studentdata">
                    <span class="date-uploaded"><p>posted on {{Projects.Uploaddate}} </p></span>
                    <div class="student-data1">
                        <p> <span><i class='bx bxs-invader' ></i></span>{{Projects.department}}</p>
                        <p> <span><i class='bx bxs-hard-hat' ></i></span>{{Projects.UserId}}</p>
    
                    </div>
                    <div class="student-data6">
                        <p><span><i class='bx bxs-invader'></i></span> {{Projects.ProjectTitle}}</p>
                        <p>
                            <span><i class='bx bx-code-alt'></i></span> 
                            {{#each Uploadpub.TechStack}}
                                    <span class="tech">{{this}}</span>
                            {{/each}}


 
                        </p>
                        <p><span><i class='bx bxl-mongodb'></i></span> {{Uploadpub.TeamName}}</p>
                    </div>
                    
                    <div class="student-data7">
                          {{#each Uploadpub.teamData}}
                        <li>
                            <div class="student-name">
                                <span><img src="/img/user.png" alt=""></span>
                                <p>{{this.name}}</p>
                            </div>
                            <div class="student-number">
                                <p>{{this.hallTicket}}</p>
                            </div>
                        </li>
                        {{/each}} 
                    </div>
                    

                    <div class="student-data2">
                         {{#each Uploadpub.Awards}} 
                        <p> <i class='bx bxs-award' ></i></span> {{this}}   <span><i class='bx bxl-google' ></i></span></p> 
                        {{/each}}
                    </div> 
                    <div class="student-data4">
                         {{#each Uploadpub.Wins}} 
                         <p><i class="ri-trophy-fill"></i> {{this}}  </p> 
                         {{/each}}
                       
                    </div>
                   
                    <div class="student-data5">
                        <p>{{Uploadpub.Description}} </p>
                    </div>
                    <div class="student-data7">
                        <h4>Start-up plans</h4>
                        
                        <p>{{Uploadpub.StartupIdea}} </p> 
                    </div>
                </div>
                

            </div>

        </div>
            
			
			
		
		<!-- MAIN -->
	</section>
	

	<script src="/js/script.js"></script>
	<script src="/js/publications.js"></script>
</body>
</html>